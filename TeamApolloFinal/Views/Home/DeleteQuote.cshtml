@model TeamApolloFinal.Models.Apollo

@{
    ViewBag.Title = "DeleteQuote";
}

<h2>DeleteQuote</h2>

@ViewBag.DeleteStatus

<h3>Are you sure you want to delete this quote?</h3>

<div>
    <p>@Model.QuoteID ) @Model.Motivation</p>
</div>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-actions no-color">
        <input type="submit" value="Delete" class="btn btn-default" /> |
        @Html.ActionLink("Back to List", "DisplayQuotes")
    </div>
}