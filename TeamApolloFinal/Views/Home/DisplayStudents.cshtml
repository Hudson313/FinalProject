@model IEnumerable<TeamApolloFinal.Models.Student>
@{
    ViewBag.Title = "DisplayStudents";
}
<div class="navbar navbar-inverse navbar-fixed-top">
    <div class="container">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            @Html.ActionLink("All Systems Go", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
        </div>
        <div class="navbar-collapse collapse">
            <ul class="nav navbar-nav">
                <li>@Html.ActionLink("View Quotes", "DisplayQuotes")</li>
                <li>@Html.ActionLink("View Students", "DisplayStudents")</li>
            </ul>
        </div>
    </div>
</div>
<h2>DisplayStudents</h2>


<body>
   
    <table style="width:100%">
        <tr>
            <th width="25%">FirstName</th>
            <th width="25%">LastName</th>
            <th width="25%">Email</th>
            <th width="25%">Track</th>
            </tr>
        </table>
            
    <table style="width:100%">
        @foreach (var Student in Model)
        {
            <tr>
                <td width="25%">@Student.FirstName</td>
                <td width="25%">@Student.LastName</td>
                <td width="25%">@Student.Email</td>
                <td width="25%">@Student.Track</td>
            </tr>
        }
    </table>
    </body>