@model IEnumerable<TeamApolloFinal.Models.Student>
@{
    ViewBag.Title = "DisplayStudents";
}

<h2>DisplayStudents</h2>


<body>
   
    <table style="width:100%">
        <tr>
            <th width="25%">FirstName</th>
            <th width="25%">LastName</th>
            <th width="25%">Email</th>
            <th width="25%">Track</th>
            </tr>
        </table>
            
    <table style="width:100%">
        @foreach (var Student in Model)
        {
            <tr>
                <td width="25%">@Student.FirstName</td>
                <td width="25%">@Student.LastName</td>
                <td width="25%">@Student.Email</td>
                <td width="25%">@Student.Track</td>
            </tr>
        }
    </table>
    </body>